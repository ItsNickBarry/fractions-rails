<h1>Character: <%= character.get("name") %></h1>
<div class="user">
  belongs to:
  <a href="<%= "#/users/" + character.user().escape("id") %>">
    <%= character.user().escape("username") %>
  </a>
</div>

<div id="fractions" class="fraction">
  <h3>Citizenship Fractions</h3>
  <div class="row">
    <div class="six columns">
      <div id="fractions-list"></div>
    </div>
    <div class="six columns">
      <div id="fractions-new"></div>
    </div>
  </div>
</div>
<div id="founded-fractions" class="fraction">
  <h3>Founded Fractions</h3>
  <div class="row">
    <div class="six columns">
      <div id="founded-fractions-list"></div>
    </div>
    <div class="six columns">
      <div id="founded-fractions-new"></div>
    </div>
  </div>
</div>
